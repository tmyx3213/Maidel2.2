{
  "name": "maidel-2.2-frontend",
  "version": "1.0.0",
  "description": "Maidel 2.2 Electron + React Frontend",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "dev": "concurrently \"npm run react\" \"wait-on http://localhost:3000 && npm run electron\"",
    "react": "react-scripts start",
    "electron": "electron .",
    "build": "react-scripts build",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "test": "react-scripts test"
  },
  "keywords": [
    "electron",
    "react",
    "adk",
    "multi-agent"
  ],
  "author": "Maidel 2.2 Project",
  "license": "MIT",
  "dependencies": {
    "@types/electron": "^1.4.38",
    "@types/node": "^24.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^9.2.1",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "4.9.5",
    "wait-on": "^9.0.0"
  },
  "overrides": {
    "typescript": "4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
